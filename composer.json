{
    "name": "codekinz/livewire-easy-tags",
    "description": "A Laravel tag system for artisans which is built in Livewire, Laravel Spatie tags, Tagify and Alpine Js.",
    "type": "library",
    "require": {
        "livewire/livewire": "^2.0.0",
        "spatie/laravel-tags": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
            "LivewireEasyTags\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Zain Farooq",
            "email": "zainfarooq541@gmail.com"
        }
    ],
    "minimum-stability": "dev-main",

    "extra": {
        "laravel": {
            "providers": [
                    "LivewireEasyTags\\LivewireEasyTagsServiceProvider"
                ]
            }
        }
}
